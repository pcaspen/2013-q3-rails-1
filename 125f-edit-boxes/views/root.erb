<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Grade</th>
  </tr>
  <% @students.each do |student| %>
    <tr>
      <td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td>
        <input type="text" value="<%= student.grade %>">
      </td>
    </tr>
  <% end %>
</table>
